  "global": {
    "security": {
      "restrictWorkingDirectory": false,
      "enableInjectionProtection": true
    }
  },
    "shells": {
      "cmd": {
        "type": "windows",
        "enabled": true,
      "executable": {
        "command": "cmd.exe",
        "args": ["/c"]
      }
    }
  }
}
