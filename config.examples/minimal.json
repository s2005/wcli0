{
  "global": {
    "security": {
      "restrictWorkingDirectory": false,
      "enableInjectionProtection": true
    }
  },
  "shells": {
    "cmd": {
      "type": "cmd",
      "enabled": true,
      "executable": {
        "command": "cmd.exe",
        "args": [
          "/c"
        ]
      }
    }
  }
}
